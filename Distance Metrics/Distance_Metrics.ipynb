      "cell_type": "code",
      "source": [
        "# Euclidean distance\n",
        "def euclidean(x, y):\n",
        "    distance = 0\n",
        "    for a, b in zip(x, y):\n",
        "        distance += (sum([(pow((a-b),2))]))\n",
        "    return sqrt(distance)\n",
        "print(\"Euclidean Distance:\",euclidean([1,3,4,1],[3,2,1,1]))"
