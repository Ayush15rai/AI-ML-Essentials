        "# Manhattan distance\n",
        "def manhattan(x, y):\n",
        "    distance=0\n",
        "    for a,b in zip(x,y):\n",
        "        distance += sum([abs(a-b)])\n",
        "    return distance\n",
        "print(\"Manhattan Distance:\",manhattan([1,3,4,1],[3,2,1,1]))"
