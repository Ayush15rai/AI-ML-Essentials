      "source": [
        "#cosine_similarity\n",
        "def cosine_similarity(x,y):\n",
        "    numerator = 0\n",
        "    sum_x = 0\n",
        "    sum_y = 0\n",
        "    for a,b in zip(x,y):\n",
        "        numerator += sum([a * b])\n",
        "        sum_x += sum([a**2])\n",
        "        sum_y += sum([b**2])\n",
        "    denominator = round(sqrt(sum_x) * sqrt(sum_y))\n",
        "    return numerator / denominator\n",
        "print(\"Cosine Similarity:\", cosine_similarity([1,3,4,1],[3,2,1,1]))"
      ],
